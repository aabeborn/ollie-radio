{
	"name": "@aabeborn/code-style",
	"version": "1.0.0",
	"description": "Code style for aabeborn projects",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"eslint.cjs",
		"prettier.js",
		"package.json"
	],
	"type": "module",
	"scripts": {
		"lint": "eslint --ext .cjs,.js,.jsx,.ts,.tsx .",
		"lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
		"lint:staged": "eslint"
	},
	"exports": {
		"./eslint": "./eslint.cjs",
		"./prettier": "./prettier.js"
	},
	"keywords": [
		"eslint",
		"prettier"
	],
	"author": "aabeborn",
	"license": "MIT",
	"devDependencies": {
		"eslint": "^8.45.0",
		"prettier": "^3.0.0"
	},
	"peerDependencies": {
		"eslint": "^8.45.0",
		"prettier": "^3.0.0"
	},
	"dependencies": {
		"@next/eslint-plugin-next": "^13.4.12",
		"@rushstack/eslint-patch": "^1.3.2",
		"@typescript-eslint/eslint-plugin": "^6.1.0",
		"@typescript-eslint/parser": "^6.1.0",
		"eslint-config-next": "^13.4.11",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-turbo": "^1.10.9",
		"eslint-import-resolver-node": "^0.3.7",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-react": "^7.33.0",
		"eslint-plugin-react-hooks": "5.0.0-canary-587ae49d7-20230720",
		"prettier-plugin-tailwindcss": "^0.4.1"
	}
}
